  },
  "devDependencies": {
    "browserify": "^10.0.0",
    "chalk": "^1.0.0",
    "diff": "^1.2.0",
    "eslint": "^0.24.0",
    "esmangle": "^1.0.0",
    "istanbul": "^0.3.0",
    "jscs": "^1.0.0",
    "jscs-jsdoc": "^1.0.0",
    "mdast": "^0.26.0",
